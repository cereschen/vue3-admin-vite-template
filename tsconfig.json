{
  "compilerOptions": {
    "module": "esNext",
    "noEmit": true,
    "allowJs": true,
    "baseUrl": ".",
    "lib": [
      "ESNext"
    ],
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "types": [
      "vite/client"
    ],
    "paths": {
      "@/*": [
        "src/*"
      ]
    }
  },
  "include": [
    "src/**/*.vue",
    "src/**/*.js",
    "src/**/*.ts",
    "src/**/*.jsx",
    "mock/**/*.js"
, "mock/user.ts", "mock/table.ts"  ],
  "exclude": [
    "node_modules"
  ]
}